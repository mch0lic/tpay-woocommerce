{
    "name": "tpay/woocommerce",
    "description": "Woocommerce payment module",
    "license": "MIT",
    "type": "woocommerce-module",
    "authors": [
        {
            "name": "tpay.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "tpay-com/tpay-openapi-php": "^1.3"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "phpunit/phpunit": "^8"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Tpay\\": "includes/"
        },
        "classmap": [
            "tpay.php"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1"
        },
        "preferred-install": "dist",
        "prepend-autoloader": false
    }
}
